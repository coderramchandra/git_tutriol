package edu.ramchandra.linkedlist;

public class MainRunner1 {
	public static void main(String[] args) {
		SinglyLinkedList sl=new SinglyLinkedList();
		sl.add(25);
		sl.add(56);
		sl.add(98);
		sl.add(78);
		sl.display();
		int size=sl.length();
		System.out.println();
		sl.addFirst(10);
		sl.addFirst(20);
		sl.display();
		Object data=sl.deleteFirst();
		System.out.println("Deleted element is: "+data);
		sl.display();
		System.out.println("========================");
		//System.out.println("Length: "+size);
		sl.deleteLast();
		sl.display();
	}
}
